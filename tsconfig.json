{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "ES2020",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "outDir": "lib"
    },
    "exclude": [
        "node_modules",
    ],
    "lib": [
        "ES2020"
    ]
}